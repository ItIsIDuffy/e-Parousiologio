{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "public/app.js"
    ],
    "headers": [
      {
        "source": "**/*.js",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=600" }
        ]
      }
    ],
    "rewrites": [
      { "source": "/c/**", "destination": "/c/index.html" },
      { "source": "**", "destination": "/index.html" }
    ]
  }
}
